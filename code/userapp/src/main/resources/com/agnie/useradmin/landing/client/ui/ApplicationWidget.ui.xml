<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:agcw='urn:import:com.agnie.gwt.common.client.widget'
	xmlns:aucw='urn:import:com.agnie.useradmin.common.widget'>

	<ui:with field='msg' type='com.agnie.useradmin.landing.client.Messages' />
	<ui:with
		type="com.agnie.useradmin.landing.client.ui.ApplicationWidgetResources"
		field="appWidgetRes"></ui:with>
	<ui:style>
		.imgDiv {
			height: 100px;
			margin: 0 auto;
			width: 150px;
		}
	</ui:style>

	<g:HTMLPanel addStyleNames="{appWidgetRes.css.appWidget}">
		<div ui:field="titleDiv" class="{appWidgetRes.css.titleDiv}"></div>
		<div class="{style.imgDiv}">
			<g:Image ui:field="img" height="100px" width="150px"></g:Image>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>