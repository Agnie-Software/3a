<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:agcw='urn:import:com.agnie.gwt.common.client.widget'>

	<ui:with field='msg' type='com.agnie.useradmin.landing.client.Messages' />

	<ui:style>
		.tab-bar {
			border-radius: 2px;
			color: #474747;
			cursor: pointer;
			float: left;
			height: 25px;
			padding: 7px 10px 2px;
			width: 180px;
			background: #e7e7e7; /* Old browsers */
			/* IE9 SVG, needs conditional override of 'filter' to 'none' */
			background:
				url(data:image/svg+xml;base64,);
			background: -moz-linear-gradient(top, #e7e7e7 0%, #c5c5c5 50%, #e7e7e7 100%);
			/* FF3.6+ */
			background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #e7e7e7),
				color-stop(50%, #c5c5c5), color-stop(100%, #e7e7e7) );
			/* Chrome,Safari4+ */
			background: -webkit-linear-gradient(top, #e7e7e7 0%, #c5c5c5 50%, #e7e7e7 100%);
			/* Chrome10+,Safari5.1+ */
			background: -o-linear-gradient(top, #e7e7e7 0%, #c5c5c5 50%, #e7e7e7 100%);
			/* Opera 11.10+ */
			background: -ms-linear-gradient(top, #e7e7e7 0%, #c5c5c5 50%, #e7e7e7 100%);
			/* IE10+ */
			background: linear-gradient(to bottom, #e7e7e7 0%, #c5c5c5 50%, #e7e7e7 100%);
			/* W3C */
			filter: progid :   DXImageTransform.Microsoft.gradient (    
				startColorstr = 
						 '#e7e7e7', endColorstr =   '#e7e7e7', GradientType =   0 );
			/* IE6-8 */
		}
	</ui:style>

	<g:HTMLPanel>
		<g:HTMLPanel ui:field="CreateNewSociety" addStyleNames="{style.tab-bar}">
			<span>
				<ui:text from="{msg.createNewApp}" />
			</span>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>